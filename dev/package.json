{
    "name": "@yuo/plui",
    "version": "0.0.2",
    "author": "Perfumere, XaDon",
    "description": "common components of Plog community",
    "files": [
        "build"
    ],
    "main": "./build/@yuo/plui.umd.js",
    "module": "./build/@yuo/plui.es.js",
    "exports": {
        ".": {
            "import": "./build/@yuo/plui.es.js",
            "require": "./build/@yuo/plui.umd.js"
        }
    },
    "keywords": [
        "plui",
        "@yuo/plui",
        "plogui"
    ],
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "lint": "eslint \"src/**/*.{js,ts,vue}\"",
        "fix": "eslint --fix \"src/**/*.{js,ts,vue}\""
    },
    "dependencies": {
        "@yuo/plui": "^0.0.2",
        "vue": "^3.0.5"
    },
    "devDependencies": {
        "@types/node": "^14.14.20",
        "@typescript-eslint/parser": "^4.12.0",
        "@vitejs/plugin-vue": "^1.0.4",
        "@vue/compiler-sfc": "^3.0.5",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vuedx/typecheck": "^0.4.1",
        "@vuedx/typescript-plugin-vue": "^0.4.1",
        "autoprefixer": "^10.2.1",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^7.0.0-0",
        "husky": "^4.3.7",
        "lint-staged": "^10.5.3",
        "postcss": "^8.2.4",
        "prettier": "^1.19.1",
        "sass": "^1.32.2",
        "typescript": "^4.1.3",
        "vite": "^2.0.0-beta.12"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.js": "eslint \"src/**/*.js\"",
        "src/**/*.vue": "eslint \"src/**/*.vue\"",
        "src/**/*.ts": "eslint \"src/**/*.ts\"",
        "src/**/*.json": "eslint \"src/**/*.ts\""
    },
    "browserslist": [
        ">1%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "license": "MIT"
}
