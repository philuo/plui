{
    "version": "0.2.0",
    "configurations": [{
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome DevTools",
        "url": "http://localhost:8080",
        "webRoot": "${workspaceFolder}/dev",
        "breakOnLoad": true,
        "sourceMapPathOverrides": {
            "webpack:///src/*": "${webRoot}/*"
        }
    }, {
        "type": "devtools-for-chrome",
        "request": "attach",
        "name": "Attach Chrome DevTools",
        "url": "http://localhost:8080/"
    }]
}
